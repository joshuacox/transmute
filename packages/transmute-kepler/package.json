{
  "name": "transmute-kepler",
  "version": "1.1.2",
  "description": "A super tiny module for querying an IPFS node that works in nodejs and the browser.",
  "main": "dist/transmute-kepler.cjs.js",
  "files": [
    "dist",
    "internals",
    "examples",
    "src"
  ],
  "scripts": {
    "start": "npm test",
    "build": "./node_modules/bili/dist/cli.js ./src/index.js",
    "test": "NODE_TLS_REJECT_UNAUTHORIZED='0' react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/transmute-industries/transmute.git"
  },
  "keywords": [
    "ipfs",
    "api",
    "add",
    "cat",
    "provider",
    "node",
    "browser"
  ],
  "authors": [
    "Orie Steele <orie@transmute.industries>"
  ],
  "engines": {
    "npm": ">=3",
    "node": ">=6.5.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/transmute-industries/transmute/issues"
  },
  "homepage": "https://github.com/transmute-industries/transmute#readme",
  "devDependencies": {
    "bili": "^3.0.4",
    "chai": "^4.1.2",
    "react-scripts": "^1.1.1"
  },
  "browser": {
    "xmlhttprequest": false,
    "./src/lib/XMLHttpRequest.js": "./src/lib/XMLHttpRequest-browser.js",
    "./lib/lib/XMLHttpRequest.js": "./lib/lib/XMLHttpRequest-browser.js"
  },
  "dependencies": {
    "xmlhttprequest": "^1.8.0"
  },
  "pre-commit": "build"
}
