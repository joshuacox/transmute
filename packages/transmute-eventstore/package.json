{
  "name": "transmute-eventstore",
  "version": "0.0.1",
  "description": "EventStore Middleware",
  "main": "dist/transmute-eventstore.min.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "npm test",
    "build": "./node_modules/bili/dist/cli.js --format cjs ./src/index.js",
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 react-scripts test --coverage --forceExit",
    "test:report": "codecov && rm -rf ./coverage",
    "test:smoke": "node ./smoke_tests/module.test.js",
    "truffle:test": "truffle test",
    "truffle:migrate": "truffle migrate",
    "truffle:test:minikube": "NODE_TLS_REJECT_UNAUTHORIZED=0 truffle test --network minikube",
    "truffle:migrate:minikube": "NODE_TLS_REJECT_UNAUTHORIZED=0 truffle migrate --network minikube",
    "truffle:coverage:minikube": "./node_modules/.bin/solidity-coverage",
    "truffle:coverage:minikube:report": "cat coverage/lcov.info | coveralls",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/transmute-industries/transmute.git"
  },
  "keywords": [
    "transmute"
  ],
  "authors": [
    "Orie Steele <orie@transmute.industries>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/transmute-industries/transmute/issues"
  },
  "homepage": "https://github.com/transmute-industries/transmute#readme",
  "devDependencies": {
    "bili": "^3.0.4",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "react-scripts": "^1.1.1",
    "solidity-coverage": "^0.4.14"
  },
  "dependencies": {
    "bs58": "^4.0.1",
    "ipfs-api": "^18.2.0",
    "keen-tracking": "^1.1.3",
    "truffle-contract": "^3.0.4",
    "web3": "^0.19.1",
    "web3-provider-engine": "^13.6.6",
    "zeppelin-solidity": "^1.6.0"
  }
}
